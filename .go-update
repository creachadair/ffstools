update_mod() {
    go get -t -u ./...
    go get \
	gvisor.dev/gvisor@v0.0.0-20240119233241-c9c1d4f9b186
}

cleanup() {
    go install github.com/creachadair/ffstools/ffs
    go install github.com/creachadair/ffstools/file2json
    go install -tags badger,gcs,s3,tailscale github.com/creachadair/ffstools/blobd
}
